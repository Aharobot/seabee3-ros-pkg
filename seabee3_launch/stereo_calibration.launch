<launch>
	<include file="$(find seabee3_launch)/down_cameras.launch" />
	<node pkg="image_sync" type="image_sync" name="image_sync" args="~image_l:=/camera3/image_raw ~image_r:=/camera4/image_raw ~info_l:=/camera3/camera_info ~info_r:=/camera4/camera_info" />
	<node pkg="camera_calibration" type="cameracalibrator.py" name="camera_calibrator" args="--square 0.048 --size 8x6 left:=/image_sync/image_l_sync right:=/image_sync/image_r_sync left_camera:=/camera3 right_camera:=/camera4" output="screen" />
</launch>