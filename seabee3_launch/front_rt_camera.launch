<launch>

	<group ns="camera_front_r" >
		<node name="camera" pkg="camera1394" type="camera1394_node">
			<param name="frame_id" type="string" value="camera_front_r" />
			<param name="bayer_pattern" type="string" value="rggb"/>
			<param name="guid" type="string" value="00b09d010090f9d-"/>
			
			<param name="camera_info_url" type="string" value="file://$(find seabee3_launch)/cfg/cfg_front_lt_camera.yaml" />
		</node>

		<group ns="camera">
			<node name="image_proc" pkg="image_proc" type="image_proc" />
		</group>
	
	</group>	
</launch>
