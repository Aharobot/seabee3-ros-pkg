<launch>
	<node name="camera1" pkg="camera1394" type="camera1394_node" output="screen">
		<rosparam param="" command="load" file="$(find seabee3_launch)/params/camera1.yaml" />
		
		<remap from="camera/camera_info" to="~/camera_info" />
		<remap from="camera/image_raw" to="~/image_raw" />
		<remap from="camera/set_camera_info" to="~/set_camera_info" />
	</node>
	<group ns="camera1">
		<node name="image_proc" pkg="image_proc" type="image_proc" output="screen" />
	</group>	
</launch>
