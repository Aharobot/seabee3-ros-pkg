<launch>
	<node name="camera3" pkg="camera1394" type="camera1394_node">
		<param name="frame_id" type="string" value="camera3" />
		<param name="bayer_pattern" type="string" value="rggb"/>
		<param name="guid" type="string" value="00b09d010090f9dc"/>
		
		<remap from="camera/camera_info" to="~/camera_info" />
		<remap from="camera/image_raw" to="~/image_raw" />
		<remap from="camera/set_camera_info" to="~/set_camera_info" />
		
		<param name="camera_info_url" type="string" value="file://$(find seabee3_launch)/cfg/camera3.yaml" />
	</node>
	<group ns="camera3">
		<node name="image_proc" pkg="image_proc" type="image_proc" />
	</group>	
</launch>
