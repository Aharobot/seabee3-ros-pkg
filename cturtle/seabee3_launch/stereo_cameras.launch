<launch>
	<!-- left -->
	<include file="$(find seabee3_launch)/camera3.launch" />
	<param name="camera3/camera_info_url" type="string" value="file://$(find seabee3_launch)/cfg/stereo_left.yaml" />
	<param name="camera3/frame_id" type="string" value="/sparse_stereo" />
	
	<!-- right -->
	<include file="$(find seabee3_launch)/camera4.launch" />
	<param name="camera4/camera_info_url" type="string" value="file://$(find seabee3_launch)/cfg/stereo_right.yaml" />
	<param name="camera4/frame_id" type="string" value="/sparse_stereo" />
</launch>
