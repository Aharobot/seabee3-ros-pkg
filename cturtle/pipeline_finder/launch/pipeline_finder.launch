<launch>

	<arg name="name_prefix" default="pipeline_finder" />
	<arg name="name_postfix" default="" />
	<arg name="pkg" value="pipeline_finder" />
	<arg name="name" value="$(arg name_prefix)$(arg name_postfix)" />
	<arg name="image" default="/color_classifier_down/images" />
	<arg name="image_transport" default="raw" />
	<arg name="node_type" value="$(arg pkg)" />

  <node pkg="$(arg pkg)" type="$(arg node_type)" name="$(arg name)" output="screen" args="~image:=$(arg image) _image_transport:=$(arg image_transport)" />
</launch>
